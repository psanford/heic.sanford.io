{
  "name": "HEIC to JPEG",
  "short_name": "HEICâ†’JPEG",
  "description": "Convert HEIC images to JPEG in the browser",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#1a1a2e",
  "theme_color": "#1a1a2e",
  "icons": [
    {
      "src": "icon-192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "icon-512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "share_target": {
    "action": "/share-target",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "files": [
        {
          "name": "file",
          "accept": ["image/heic", "image/heif", ".heic", ".heif"]
        }
      ]
    }
  }
}
